function countPrime(n){
function test_prime(n)
{

  if (n===1)
  {
    return false;
  }
  else if(n === 2)
  {
    return true;
  }else
  {
    for(var x = 2; x < n; x++)
    {
      if(n % x === 0)
      {
        return false;
      }
    }
    return true;  
  }
}
count=0;
for(i=2;i<n;i++){
    if(test_prime(i)){
        ++count;
    }
}
return count;

}

countPrime(10);
